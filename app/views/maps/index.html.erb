        title: 'searchAreaMarker'
    });

        // drawing polygon
var polygonCoords = [
  new google.maps.LatLng(25.725848, -100.201932),
  new google.maps.LatLng(25.722368, -100.203713),
  new google.maps.LatLng(25.724688, -100.217232),
  new google.maps.LatLng(25.723606, -100.224656 )
];


// Construct the polygon.
drawingPolygon(polygonCoords);

function drawingPolygon(polygonCoords) {
  var polygon = new google.maps.Polygon({
    paths: polygonCoords,
    strokeColor: '#FF00FF',
    strokeOpacity: 0.8,
    strokeWeight: 2,
    fillColor: '#FF0000',
    fillOpacity: 0.35,
    draggable:true,
    editable: true
  });
  polygon.setMap(map);
}

